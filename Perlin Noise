float xnoise = 0.0;
float ynoise = 0.0;
float inc = 0.1;

void setup() {
  size(700, 100);
  noLoop();
  noSmooth();
}

void draw() {
  background(0);
  for (int y = 0; y < height; y++) {// column
    for (int x = 0; x < width; x++) { //row
      float gray = noise(xnoise, ynoise) * 255;
      stroke(gray);
      point(x, y);
      xnoise = xnoise + inc;
    }
    xnoise = 0;
    ynoise = ynoise + inc;
  }
}
